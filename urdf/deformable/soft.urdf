<?xml version="1.0" ?>
<robot name="isosphere">
	<link name="ball">    
		<collision>
			<origin xyz="0 0 0" />
			<geometry>
				<sphere radius="0.002" />
			</geometry>
		</collision>
		<inertial>
			<mass value="0.3" />
			<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
		</inertial>
	</link>

	<joint name = "attach" type = "revolute">
		<origin xyz = "0.0 0.0 0.0" rpy = "0 0 0"/>
		<parent link = "ball"/>
		<child link = "soft"/>
		<limit effort="10000" velocity="0.01" lower="-0.0005" upper="0.0005"/>
	</joint>

	<link name="soft">
		<fem>
			<origin rpy="0.0 0.0 0.0" xyz="0 0 0"/>
			<density value="1e7"/>
			<youngs value="1e7"/>
			<poissons value="0"/>
			<damping value="0"/>
			<tetmesh filename="icosphere.tet"/>
			<scale value="0.007"/>
		</fem>
	</link> 
</robot>
